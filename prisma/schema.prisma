datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Room {
  id Int @id @default(autoincrement())
  display Int 
  table Int
}

model Block {
  id Int @id
  room_id Int 
  booked_user_id Int 
} 

model User {
  id Int @id @default(autoincrement())
  username String 
  password String
}
