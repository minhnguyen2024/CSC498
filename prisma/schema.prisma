datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Room {
  id Int @id @default(autoincrement())
  accessible Int
  power Int
  reservable Int
  softSeating Int
  tableChairs Int
  monitor Int 
  whiteboard Int
  window Int
}

model Block {
  id Int @id @default(autoincrement())
  room_id Int 
  time Int
  booked_user_id Int 
} 

model User {
  id Int @id @default(autoincrement())
  username String 
  password String
  admin Int
}
